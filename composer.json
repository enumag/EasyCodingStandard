{
    "name": "symplify/easy-coding-standard",
    "description": "Prefixed scoped version of ECS package",
    "license": "MIT",
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/easy-testing": "^9.3",
        "symplify/smart-file-system": "^9.3"
    },
    "autoload": {
        "files": [
            "bootstrap.php"
        ]
    },
    "bin": [
        "bin/ecs"
    ],
    "conflict": {
        "squizlabs/php_codesniffer": "<3.6",
        "friendsofphp/php-cs-fixer": "<3.0"
    },
    "config": {
        "vendor-dir": "vendor-tests"
    }
}
